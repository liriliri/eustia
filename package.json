{
  "name": "eustia",
  "version": "1.0.0",
  "description": "Tool for generating utility libraries",
  "main": "out/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/liriliri/eustia.git"
  },
  "bin": {
    "eustia": "./bin/eustia.js"
  },
  "scripts": {
    "dev": "copyfiles -u 1 src/**/*.js out && tsc -w --inlineSourceMap",
    "build": "copyfiles -u 1 src/**/*.js out && tsc",
    "lint": "tslint src/**/*.ts",
    "lint:fix": "tslint src/**/*.ts --fix",
    "test": "tsc --inlineSourceMap && nyc mocha",
    "ci": "npm run lint && npm run build && npm test",
    "format": "prettier \"src/**/*.ts\" \"bin/*.js\" \"test/**/*.js\" \"*.{json,js}\" --write"
  },
  "keywords": [
    "utility",
    "library",
    "custom",
    "javascript",
    "underscore"
  ],
  "author": "redhoodsu",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/liriliri/eustia/issues"
  },
  "homepage": "https://github.com/liriliri/eustia#readme",
  "dependencies": {
    "async": "^2.6.0",
    "axios": "^0.19.0",
    "chokidar": "^2.0.0",
    "fs-extra": "^8.0.0",
    "glob": "^7.1.2",
    "handlebars": "^4.7.6",
    "marked": "^0.3.9",
    "nopt": "^4.0.1",
    "qs": "^6.5.1",
    "request": "^2.88.0",
    "request-promise": "^4.2.4",
    "source-map": "^0.7.3"
  },
  "devDependencies": {
    "@types/async": "^2.0.49",
    "@types/chalk": "^2.2.0",
    "@types/chokidar": "^1.7.5",
    "@types/fs-extra": "^5.0.4",
    "@types/glob": "^5.0.35",
    "@types/marked": "^0.4.1",
    "@types/node": "^10.9.4",
    "@types/nopt": "^3.0.29",
    "@types/qs": "^6.5.1",
    "@types/request-promise": "^4.1.42",
    "chai": "^4.1.2",
    "copyfiles": "^2.1.0",
    "istanbul": "^0.4.5",
    "mocha": "^4.1.0",
    "nyc": "^15.1.0",
    "prettier": "^1.19.1",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "typescript": "^4.0.5"
  }
}
